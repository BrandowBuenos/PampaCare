datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model raca {
  id   Int    @id @default(autoincrement())
  nome String @unique
}

model sexo {
  id   Int    @id @default(autoincrement())
  nome String @unique
}

model bairro {
  id   Int    @id @default(autoincrement())
  nome String @unique
}

model area {
  id   Int    @id @default(autoincrement())
  nome String @unique
}